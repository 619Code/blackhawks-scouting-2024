          "default": 3
        },
        {
          "name": "Failed Charging Station",
          "type": "checkbox"
        },
        {
          "name": "Robots On Charge Station",
          "type": "spinbox",
          "max": 3
        },
        {
          "name": "Mechanical Failures",
          "type": "text"
        },
        {
          "name": "Comments",
          "type": "textarea"
        }
      ]
    }
